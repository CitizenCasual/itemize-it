<% profiles.forEach(profile => { %>
  <a href="/profiles/<%= profile._id %> ">View Profile</a>
  <% if (profile.avatar) { %> 
    <img src="<%= profile.avatar %> ">  
  <% } %> 
  <p><%= profile.name %> </p>
<% }) %>  

